{
  "version": 2,
  "functions": {
    "api/bihinkanri-proxy.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "routes": [
    {
      "src": "/api/bihinkanri-proxy",
      "dest": "/api/bihinkanri-proxy.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization, X-Account-ID" }
      ]
    }
  ]
} 